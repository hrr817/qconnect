 const { room_name, members, isPrivate, admin } = doc.data();\n          temp.push({\n            id: doc.id,\n            name: room_name,\n            members: members,\n            private: isPrivate,\n            admin,\n          });\n        });\n        setRooms([...temp]);\n      });\n\n    return () => {\n      clearTimeout(timeout);\n    };\n  }, [dispatch]);\n\n  return (\n    <>\n      {loading && <Loading />}\n      <Navbar />\n      <div className=\"container max\">\n        {/* Chat Room */}\n        <ChatRoom currentRoom={currentRoom} />\n        {/* List of rooms */}\n        <RoomsList\n          rooms={rooms}\n          currentRoom={currentRoom}\n          setCurrentRoom={setCurrentRoomCallBack}\n        />\n      </div>\n      {/* <section>\n        {user ? (\n          <>\n            <CreateRoomForm user={user} />\n            <button onClick={() => app.auth().signOut()}>Sign Out</button>\n          </>\n        ) : (\n          <SignInForm setUser={setUserCallback} />\n        )}\n      </section> */}\n    </>\n  );\n}\n\nexport default App;\n","/home/kirin/workspace/qconnect/src/firebaseConfig.js",[],"/home/kirin/workspace/qconnect/src/components/CreateRoomForm.js",[],"/home/kirin/workspace/qconnect/src/components/SignInForm.js",[],"/home/kirin/workspace/qconnect/src/components/Navbar.js",[],"/home/kirin/workspace/qconnect/src/components/ChatRoom.js",[],"/home/kirin/workspace/qconnect/src/components/RoomsList.js",[],"/home/kirin/workspace/qconnect/src/components/Message.js",[],"/home/kirin/workspace/qconnect/src/components/InputBar.js",[],"/home/kirin/workspace/qconnect/src/components/Modal.js",[],"/home/kirin/workspace/qconnect/src/store.js",[],"/home/kirin/workspace/qconnect/src/features/userSlice.js",[],"/home/kirin/workspace/qconnect/src/components/Loading.js",[],{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","replacedBy":"70"},{"ruleId":"71","severity":1,"message":"72","line":15,"column":8,"nodeType":"73","messageId":"74","endLine":15,